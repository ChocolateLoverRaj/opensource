  },
  "licenseStart": "2018",
  "verb": {
    "data": {
      "depsType": "dev",
      "directory": "packages/parse-commit-message",
      "avatar": "https://avatars3.githubusercontent.com/u/5038030?v=4",
      "twitter": "tunnckoCore",
      "github": "tunnckoCore"
    },
    "readme": "../../config/readme-template.md",
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
