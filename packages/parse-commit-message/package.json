  "engines": {
    "node": "^8.10.0 || >=10.13.0"
  },
  "main": "dist/",
  "module": "dist/",
  "files": [
    "dist"
  ],
  "keywords": [],
  "scripts": {
    "build": "BABEL_ENV=build rolldown --pkg-dir $PWD",
