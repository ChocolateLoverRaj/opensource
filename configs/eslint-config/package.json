{
  "name": "@tunnckocore/eslint-config",
  "version": "5.2.4",
  "licenseStart": 2019,
  "license": "MPL-2.0",
  "description": "Shareable ESLint config for all @tunnckoCore projects",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com> (https://tunnckocore.com)",
  "homepage": "https://tunnckocore.com/opensource",
  "funding": "https://ko-fi.com/tunnckoCore/commissions",
  "repository": {
    "type": "git",
    "url": "https://github.com/tunnckoCore/opensource.git",
    "directory": "configs/eslint-config"
  },
  "main": "index.js",
  "module": "index.js",
  "files": [
    "*.js"
  ],
  "engines": {
    "node": ">=10.13"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {},
  "dependencies": {
    "@tunnckocore/utils": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "babel-eslint": "^10.0.3",
    "buntis": "^0.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-config-xo-react": "^0.22.0",
    "eslint-config-xo-typescript": "^0.24.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-mdx": "^1.6.5",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-mdx": "^1.6.5",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-unicorn": "^15.0.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.8.3",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "cov": {
    "color": "grey"
  },
  "keywords": [
    "airbnb codestyle",
    "babel-preset",
    "browser config",
    "browser-config",
    "browserslist",
    "browserslist-config",
    "browserslist-preset",
    "chrome",
    "config",
    "devest",
    "env",
    "eslint-config",
    "eslint-plugin",
    "esmodules",
    "evergreen",
    "hela",
    "jsx",
    "preset-env",
    "prettier",
    "prettier-config",
    "promises",
    "react",
    "react hooks",
    "shareable-configs",
    "tunnckocorehq",
    "typescript",
    "typescript-eslint",
    "unicorn",
    "xo",
    "xo-config"
  ],
  "verb": {
    "readme": "../../readme-template.md",
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [],
    "related": {
      "list": []
    }
  }
}
